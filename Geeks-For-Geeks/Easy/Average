// User function Template for C++

int posAverage(int arr[], int size)
{
    int sum = 0 ;
    int size2 = 0 ;
    for(int i = 0 ; i< size ; i++)
    {
        if(arr[i]>=0)
        { 
            sum += arr[i];
            size2++;
        }
        
    }
    if(size2 == 0) return 0;
    return sum/size2;
     
}
