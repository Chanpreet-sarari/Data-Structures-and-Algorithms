class Solution {
  public:
    vector<int> kLargest(vector<int>& arr, int k) {
         sort(arr.begin(),arr.end());
         int n = arr.size();
         vector<int> ans ;
          for(int i = 0 ; i< k ; i++)
          {
              ans.push_back(arr[n-1-i]);
          }
          return ans;
    }
};
